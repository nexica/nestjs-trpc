{
    "$schema": "https://turborepo.com/schema.json",
    "ui": "tui",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**", ".next/**"]
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "start": {
            "cache": false
        },
        "lint": {
            "dependsOn": ["^lint"]
        }
    }
}
