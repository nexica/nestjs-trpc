{
    "semanticCommits": "enabled",
    "labels": ["dependencies"],
    "packageRules": [
        {
            "matchUpdateTypes": [
                "patch",
                "pin",
                "pinDigest",
                "digest",
                "lockFileMaintenance"
            ],
            "rangeStrategy": "bump",
            "automerge": true
        }
    ],
    "extends": ["config:recommended"]
}
