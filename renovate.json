{
    "semanticCommits": "enabled",
    "labels": ["dependencies"],
    "packageRules": [
        {
            "rangeStrategy": "bump",
            "automerge": true
        }
    ],
    "extends": [
        ":dependencyDashboard",
        ":semanticPrefixFixDepsChoreOthers",
        ":ignoreModulesAndTests",
        "group:monorepos",
        "group:recommended",
        "mergeConfidence:age-confidence-badges",
        "replacements:all",
        "workarounds:all"
      ]
}
